package com.prcatice;

import java.util.Scanner;

public class PracticeFirstPrograme {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);

		System.out.println(" Enter value NO :  1 ");
		int a = scanner.nextInt();

		System.out.println(" Enter value NO :  2 ");
		int b = scanner.nextInt();

		System.out.println(" Enter value NO :  3 ");
		int c = scanner.nextInt();

		scanner.close();

		if (a > b && a > c) {
			// if (a > c )
			System.out.println(a + "  is Gretest NUm ");
			
		} else if (b > a && b > c) {
			if (b > c)
				System.out.println(b + "  Is Gretest Num");
		}
		else if (c > a && c > b ) {
			// if (c > b )
			System.out.println(c + "  is Gretest Num");
		}

	}

}
